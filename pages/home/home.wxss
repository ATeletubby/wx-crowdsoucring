/* pages/home/home.wxss */
.topbar{
  
}
.top-searchbar {
  background-color: #F8D34E !important;
}
.navbar{
  position: relative;
  display:flex;
  background: #ffffff;
  flex-wrap: wrap;
}
.navbar span{
  color:#B8B8B8;
}
.navbar_item{
  position: relative;
  display: block;
  flex: 1;
  padding: 14rpx 0;
  text-align: center;
  font-size: 24rpx;
  color:#737C93;
}
.navbar_item.navbar_item_selected{
  color:#000;
}
.navbar_item_icon{

}
.topbar_content{
  position: absolute;
  padding: 20rpx 0;
  background-color: #ffffff;
  z-index: 1;
}
.topbar_tag{
  display: flex;
  flex-wrap:wrap;
  justify-content: space-between;
  align-content: flex-start;
}
.topbar_tag button{
  display: inline-block;
  flex: 0 0 auto;
  width: 200rpx;
  height:60rpx;
  background: none;
  margin-bottom: 20rpx;
  padding:0;
  border:2rpx solid #ccc;
  border-radius: 10rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color:#737C93;
}
.topbar_button{
  display: flex;
  margin-top: 50rpx;
}
.topbar_button button{
  border-radius: 5%;
  font-size: 24rpx;
  color:#737C93;
}
.topbar_tag button.topbar_tag_selected{
  background-color: #FFF1BE;
  border: none;
}

.list{
  padding: 30rpx;
  /* display: flex; */
}
button.typebutton{
  width: 40% !important; 
}
